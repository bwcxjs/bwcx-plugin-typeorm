    "bwcx-common": "^1.0.1",
    "bwcx-core": "^1.0.1",
    "bwcx-ljsm": "^1.0.2",
    "copyfiles": "^2.4.1",
    "jest": "^26.6.3",
    "prettier": "^2.5.1",
